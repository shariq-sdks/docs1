<% for (const version of [...config.supportedVersions[serverType]].reverse()) { -%>
<%- include('option.ejs', {
  value: `${serverType}/${version}`,
  label: `${translations.serverTypes[serverType]}@${version}`,
  selected: (serverType === req.params.serverType && version === req.versionInfo.version)
}); %>
<% }; -%>